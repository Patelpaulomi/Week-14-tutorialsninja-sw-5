<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="testsuite" verbose="1">
    <listeners>
        <listener class-name="com.tutorialsninja.customlisteners.CustomListeners"></listener>
        <listener class-name="org.uncommons.reportng.HTMLReporter"></listener>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"></listener>
    </listeners>
    <test name="All Test">
        <classes>
            <class name="com.tutorialsninja.testsuite.AccountLoginPageTest"></class>
            <class name="com.tutorialsninja.testsuite.AccountRegisterTest"></class>
            <class name="com.tutorialsninja.testsuite.DesktopTest"></class>
            <class name="com.tutorialsninja.testsuite.LaptopAndNotebooksTest"></class>
            <class name="com.tutorialsninja.testsuite.TopMenuTest"></class>
        </classes>
    </test>
</suite>